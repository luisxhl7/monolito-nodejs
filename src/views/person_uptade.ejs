<%- include('partials/_header.ejs'); -%>
<div class="container mt-5">
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/update/<%= data.id %>" method="post">
                        <div class="form-group mt-2">
                            <input type="text" name="name" value="<%= data.name %>" class="form-control">
                        </div>
                        <div class="form-group mt-2">
                            <input type="text" name="lastname" value="<%= data.lastname %>" class="form-control">
                        </div>
                        <div class="form-group mt-2">
                            <input type="text" name="documentType" value="<%= data.documentType %>" class="form-control">
                        </div>
                        <div class="form-group mt-2">
                            <input type="text" name="documentNumber" value="<%= data.documentNumber %>" class="form-control">
                        </div>
                        <div class="form-group mt-2">
                            <input type="text" name="age" value="<%= data.age %>" class="form-control">
                        </div>
                        <div class="form-group mt-2 mb-2">
                            <input type="text" name="cityOfBirth" value="<%= data.cityOfBirth %>" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-primary"> SAVE </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<%- include('partials/_footer.ejs'); -%>
